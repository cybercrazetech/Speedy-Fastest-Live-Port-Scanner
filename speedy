def One():
    for i in range(1,771):
        with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
            if s.connect_ex((ip, i)) == 0:print("Port",str(i),"is open");check.append(i)
        if i%100==0:print(str(int(i*100/771))+r"% complete")


