    threads.append(threading.Thread(target=Seventyfour))
    threads.append(threading.Thread(target=Seventyfive))
    threads.append(threading.Thread(target=Seventysix))
    threads.append(threading.Thread(target=Seventyseven))
    threads.append(threading.Thread(target=Seventyeight))
    threads.append(threading.Thread(target=Seventynine))
    threads.append(threading.Thread(target=Eighty))
    threads.append(threading.Thread(target=Eightyone))
    threads.append(threading.Thread(target=Eightytwo))
    threads.append(threading.Thread(target=Eightythree))
    threads.append(threading.Thread(target=Eightyfour))
    threads.append(threading.Thread(target=Eightyfive))
    for thread in threads:thread.start()
    for thread in threads:thread.join()
    if check!=[]:
        print("#Checking for false positives...")
        for port in check:
            if checkport(ip,port):confirmed.append(str(port))
        if confirmed!=[]:print("Port",','.join(confirmed),"are open!")
        else:print("No Open Port Found!")
    else:print("No Open Port Found!")
elif choice=='2':
    port=input(
'''
check port
>''')
    if checkport(ip,port):print("Port",port,"is open")
    else:print("Port",port,"is not open")
else:print("Unknown input. Please try again.")